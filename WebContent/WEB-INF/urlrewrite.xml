<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>
    <rule>
        <condition type="request-uri" operator="equal" next="or">/$</condition>
        <condition type="request-uri" operator="equal" next="or">/safeGuarantee$</condition>
        <condition type="request-uri" operator="equal" next="and">/about$</condition>
	    <condition type="scheme" operator="equal">https</condition>
	    <from>^/(.*)</from>
	    <to type="redirect">http://%{server-name}:80%{context-path}/$1</to>
	</rule>
    <rule>
        <condition name="host" operator="notequal">^www.hmjr99.com</condition>
        <condition name="host" operator="notequal">^192.168.41.167</condition>
        <condition name="host" operator="notequal">^localhost</condition>
        <condition name="host" operator="notequal">^116.228.234.237</condition>
        <from>^/(.*)</from>
        <to type="permanent-redirect">%{scheme}://www.hmjr99.com:%{port}/$1</to>
    </rule>
	<rule><!--债权转让-->
		<from>^/creditassignment</from>
		<to>/borrow/toBorrowTransList.do</to>
	</rule>
	<rule><!--关于我们-->
		<from>^/about</from>
		<to>/content/channelDetail/62.do</to>
	</rule>
	<outbound-rule>
        <from>/content/channelDetail/62.do</from>
        <to>/about</to>
    </outbound-rule>
	<rule><!--平台介绍-->
		<from>^/introduction</from>
		<to>/content/channelDetail/62.do</to>
	</rule>
	<rule><!--安全保障-->
		<from>^/security</from>
		<to>/content/contentList/0_29.html</to>
	</rule>
	<outbound-rule>
        <from>/content/contentList/0/29.do</from>
        <to>/security</to>
    </outbound-rule>
	<rule><!--资讯中心-->
		<from>^/zixun$</from>
		<to>/content/contentList/0_76.html</to>
	</rule>
	<rule><!-- 资讯详情页面  -->
		<from>^/zixun/(.*)</from>
		<to>/content/contentDetails/76_$1.html</to>		
	</rule>
	<outbound-rule>
        <from>/content/contentList/0/76.do</from>
        <to>/zixun</to>
    </outbound-rule>
	<rule><!--专题中心-->
		<from>^/zhuanti$</from>
		<to>/channel/channelList/0_77.html</to>
	</rule>
	<rule><!-- 专题详情页面  -->
		<from>^/zhuanti/([0-9]+)</from>
		<to>/content/contentList/77_$1.html</to>		
	</rule>
	<rule><!-- 专题文章详情页面  -->
		<from>^/zhuanti/([0-9]+)_([0-9]+)</from>
		<to>/content/contentDetails/77_$1_$2.html</to>		
	</rule>
	<rule>
        <from>^/content/contentDetails/([0-9]+)_([0-9]+)_([0-9]+).html</from>
        <to>/content/contentDetails/$1/$2/$3.do</to>
    </rule>
    <outbound-rule>
        <from>/content/contentList/0/77.do</from>
        <to>/zhuanti</to>
    </outbound-rule>
	<rule><!--浩茗公告-->
		<from>^/notice</from>
		<to>/content/contentList/0_35.html</to>
	</rule>
    <outbound-rule>
        <from>/content/contentList/0/35.do</from>
        <to>/notice</to>
    </outbound-rule>
	<rule><!--媒体报告-->
		<from>^/report</from>
		<to>/content/contentList/0_27.html</to>
	</rule>
    <outbound-rule>
        <from>/content/contentList/0/27.do</from>
        <to>/report</to>
    </outbound-rule>
	<rule><!--联系我们-->
		<from>^/contact</from>
		<to>/content/contentList/0_48.html</to>
	</rule>
    <outbound-rule>
        <from>/content/contentList/0/48.do</from>
        <to>/contact</to>
    </outbound-rule>
	<rule><!--新手指引-->
		<from>^/guide</from>
		<to>/commons/guide.do</to>		
	</rule>
	<rule><!--安全保障-->
		<from>^/safeGuarantee</from>
		<to>/commons/safeGuarantee.do</to>		
	</rule>
	<rule><!--我要理财-->
		<from>^/invest$</from>
		<to>/borrow/toBorrowInfos.do</to>		
	</rule>	
	<rule><!-- 理财产品页面规则  -->
		<from>^/invests/(.*)</from>
		<to>/borrow/showBorrow/$1.do</to>		
	</rule>
    <rule>
        <from>^/([a-zA-Z]+).html</from>
        <to>/$1.do</to>
    </rule>
    <rule>
        <from>^/account/(.*).html</from>
        <to>/account/$1.do</to>
    </rule>
    <rule>
        <from>^/basics/(.*).html</from>
        <to>/basics/$1.do</to>
    </rule>
    <rule>
        <from>^/borrow/(.*).html</from>
        <to>/borrow/$1.do</to>
    </rule>
    <rule>
        <from>^/commons/(.*).html</from>
        <to>/commons/$1.do</to>
    </rule>
    <rule>
        <from>^/content/channelDetail/(.*).html</from>
        <to>/content/channelDetail/$1.do</to>
    </rule>
    <outbound-rule>
        <from>/content/channelDetail/([0-9]+).do</from>
        <to>/content/channelDetail/$1.html</to>
    </outbound-rule>
    <rule>
        <from>^/content/contentDetail/([0-9]+).html</from>
        <to>/content/contentDetail/$1.do</to>
    </rule>
    <rule>
        <from>^/content/contentDetails/([0-9]+)_([0-9]+).html</from>
        <to>/content/contentDetails/$1/$2.do</to>
    </rule>
    <outbound-rule>
        <from>/content/contentDetails/([0-9]+)/([0-9]+).do</from>
        <to>/content/contentDetails/$1_$2.html</to>
    </outbound-rule>
    <rule>
        <from>^/content/contentList/([0-9]+)_([0-9]+).html</from>
        <to>/content/contentList/$1/$2.do</to>
    </rule>
    <outbound-rule>
        <from>/content/contentList/([0-9]+)/([0-9]+).do</from>
        <to>/content/contentList/$1_$2.html</to>
    </outbound-rule>
    <rule>
        <from>^/content/(.*).html</from>
        <to>/content/$1.do</to>
    </rule>
    <rule>
        <from>^/invest/(.*).html</from>
        <to>/invest/$1.do</to>
    </rule>
    <rule>
        <from>^/member/(.*).html</from>
        <to>/member/$1.do</to>
    </rule>
    <rule>
        <from>^/quickBorrow/(.*).html</from>
        <to>/quickBorrow/$1.do</to>
    </rule>
    <rule>
        <from>^/redenvelopes/(.*).html</from>
        <to>/redenvelopes/$1.do</to>
    </rule>
    <rule>
        <from>^/shop/(.*).html</from>
        <to>/shop/$1.do</to>
    </rule>
    <rule>
        <from>^/usrOrder/(.*).html</from>
        <to>/usrOrder/$1.do</to>
    </rule>
    <rule>
        <from>^/verify/(.*).html</from>
        <to>/verify/$1.do</to>
    </rule>
    <rule><!--注册-->
    	<from>^/register$</from>
    	<to>/toRegister.do</to>
    </rule>
    <rule><!--登录-->
    	<from>^/login$</from>
    	<to>/toLogin.do</to>
    </rule>
    <rule><!--帮助中心-->
    	<from>^/help</from>
    	<to>/content/cpjs.do</to>
    </rule>
    <rule><!--忘记密码-->
    	<from>^/resetPassword$</from>
    	<to>/toResetPassword.do</to>
    </rule>
    <rule>
        <from>^/channel/channelList/([0-9]+)_([0-9]+).html</from>
        <to>/channel/channelList/$1/$2.do</to>
    </rule>
</urlrewrite>